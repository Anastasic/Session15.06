        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:МагазинчикУДома"
        mc:Ignorable="d"
        Title="ADDWindow" Height="350" Width="400">
    <Grid>
        <TextBox x:Name="nam" HorizontalAlignment="Left" Height="23" Margin="120,54,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pr" HorizontalAlignment="Left" Height="23" Margin="120,106,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="165,193,0,0" VerticalAlignment="Top" Width="75" Click="AddClick"/>
        <ComboBox x:Name="cat" HorizontalAlignment="Left" Margin="120,166,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
